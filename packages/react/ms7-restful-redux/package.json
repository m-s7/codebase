{
  "private": true,
  "name": "@ms7/restful-redux",
  "version": "0.0.1",
  "description": "Service for restful api calls using redux reducer and axios",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "build": "node_modules/.bin/tsup src/index.ts --format esm,cjs --dts --external react",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "tsup": "^6.0.1",
    "eslint": "^8.16.0",
    "react": "^18.0.0",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "typescript": "^4.6.3",
    "@reduxjs/toolkit": "^1.8.1",
    "react-redux": "^8.0.1",
    "@types/react-redux": "^7.1.24",
    "redux-thunk": "^2.4.1",
    "axios": "^0.27.1"
  },
  "devDependencies": {
    "ts-config-lib": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
