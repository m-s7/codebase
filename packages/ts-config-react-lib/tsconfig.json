{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "react library",
  "extends": "ts-config-react/tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "ES2015"],
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "noEmit": true,
    "declaration": true,
    "declarationMap": true
  },
}
