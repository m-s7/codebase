{
  "private": true,
  "name": "@ms7/websocket",
  "version": "0.0.1",
  "description": "Websocket client with multiple protocol support and react integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "node_modules/.bin/tsup src/index.ts --format esm,cjs --dts",
    "dev": "node_modules/.bin/tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sockjs-client": "^1.5.1",
    "eslint": "^8.20.0",
    "react": "^18.0.0",
    "sockjs-client": "^1.6.0",
    "tsup": "^6.2.0",
    "typescript": "^4.7.4",
    "webstomp-client": "^1.2.6"
  },
  "devDependencies": {
    "ts-config-lib": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
