{
  "private": true,
  "name": "@ms7/bui",
  "version": "0.0.1",
  "description": "BUI (bootstrap UI) contains React UI components styled with bootstrap library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "build": "node_modules/.bin/tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "node_modules/.bin/tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@ms7/common": "*",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "bootstrap": "^5.1.3",
    "eslint": "^8.16.0",
    "react": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "tsup": "^6.0.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "eslint-plugin-storybook": "^0.5.13",
    "jest": "^28.1.1",
    "jest-each": "^28.0.1",
    "jest-environment-jsdom": "^28.1.0",
    "jest-svg-transformer": "^1.0.0",
    "ts-config-lib": "*",
    "ts-jest": "^28.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
