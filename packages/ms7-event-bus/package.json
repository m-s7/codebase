{
  "name": "@ms7/event-bus",
  "description": "Event bus for JavaScript",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "node_modules/.bin/tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "node_modules/.bin/tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "tsup": "^6.0.1",
    "eslint": "^8.16.0",
    "react": "^18.0.0",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "typescript": "^4.6.3",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.182",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "ts-config-lib": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
